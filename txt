originAnimOverwrite = readBytes(param,0x38)
local anim = {
   grav = {
      717040000,
      717040005,
      717040010,
      860040000,
   },
   fire = {
      663040000,
      663040010,
      881040000,
      881040010,
      884040000,
      884040001,
      884040005,
      884040006,
   },
}
local animCheck = 0
local animTimer = createTimer(getMainForm())
animTimer.Interval = 100
animTimer.OnTimer = function()
   local animation = readInteger("[[[[[WorldChrMan]+10EF8]+0]+190]+18]+20")
   if animation ~= animCheck then
      animCheck = animation
      local time = os.date("%X")
      printf("[%s] animation: %d",time,animation)
   end
   if inArray(anim.grav,animation) then
      local bytes = { 0x00,0x00,0x00,0x40,0xAC,0xE9,0x23,0x50,0xBE,0x89,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0xFF,0xFF,0xFF,0x64,0x28,0x64,0x0A,0x0A,0x0A,0x14,0x14,0x14,0x14,0x14,0x14,0x48,0xB4,0xCC,0xED,0x6A,0x7B,0xB5,0x80,0x3F,0x00,0x00,0x01,0x00,0x30,0x67,0xE6,0x37,0xA9,0x30,0x6D,0x8C, }
      writeBytes(param,bytes)
   elseif inArray(anim.fire,animation) then
      local bytes = { 0xEC,0x51,0x38,0x3E,0x42,0x60,0xE5,0x3B,0x9A,0x99,0x19,0x3E,0xCD,0xCC,0xCC,0x3D,0x00,0x00,0x00,0x00,0xDC,0x5A,0x14,0x86,0xD8,0xD8,0xDC,0xDC,0x00,0xFF,0xB4,0x00,0x53,0xEC,0x53,0x00,0x00,0x00,0x80,0x3F,0x00,0x00,0x80,0x3F,0x00,0x00,0x01,0x00,0x5C,0x8F,0xC2,0x3E,0x00,0x00,0x80,0x3F, }
      writeBytes(param,bytes)
   else
      --local bytes = { 0x9A,0x99,0x99,0x3E,0x8F,0xC2,0xF5,0x3C,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x3F,0x78,0x78,0x78,0x2D,0x2D,0x2D,0x0A,0x0A,0x0A,0x14,0x14,0x14,0x14,0x14,0x14,0x00,0x00,0x00,0x80,0x3F,0x00,0x00,0x80,0x3F,0x00,0x00,0x01,0x00,0x5C,0x8F,0xC2,0x3E,0x00,0x00,0x00,0x00, }
      --local bytes = { 0xEC,0x51,0x38,0x3E,0x42,0x60,0xE5,0x3B,0x9A,0x99,0x19,0x3E,0xCD,0xCC,0xCC,0x3D,0x00,0x00,0x00,0x00,0xBE,0x14,0x2D,0x86,0xD8,0xD8,0xDC,0xDC,0x00,0xFF,0xB4,0x00,0x53,0xEC,0x53,0x00,0x00,0x00,0x80,0x3F,0x00,0x00,0x80,0x3F,0x00,0x00,0x01,0x00,0x5C,0x8F,0xC2,0x3E,0x00,0x00,0x80,0x3F, }
      local bytes = { 0x9A,0x99,0x19,0x3F,0x00,0x00,0x00,0x00,0xCD,0xCC,0x4C,0x3E,0xCD,0xCC,0x4C,0x3E,0x00,0x00,0x00,0x00,0xFF,0x00,0x00,0x00,0x00,0x00,0xFF,0xFF,0xFF,0xFF,0xFF,0xFF,0x00,0x00,0x00,0x00,0x00,0x00,0x80,0x3F,0x00,0x00,0x80,0x3F,0x00,0x00,0x01,0x00,0x5C,0x8F,0xC2,0x3E,0x00,0x00,0x00,0x00, }
      writeBytes(param,bytes)
   end
   if not memrec.Active or readQword("WorldChrMan") == nil then
      animTimer.destroy()
   end
end
